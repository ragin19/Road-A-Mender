<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sih.potholeapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:name=".firebase"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".workermarkerclick"></activity>
        <activity android:name=".civilianmarkerclick" />
        <activity android:name=".WorkerRegistrationActivity" />
        <activity android:name=".CivilianRegistrationActivity" />
        <activity android:name=".ImagesActivity" />
        <activity android:name=".ChooseLocation" />
        <activity android:name=".ChooseImage" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".workerActivity" />
        <activity android:name=".homescreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".loginpage" />
        <activity android:name=".SecondActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key" />

        <activity android:name=".MainActivity" />
    </application>

</manifest>